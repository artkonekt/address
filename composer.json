{
    "name": "konekt/address",
    "description": "Laravel module for handling countries, cities, addresses, people and organizations",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/address"
    },
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "doctrine/dbal": "^3.5.1|^4.0",
        "konekt/concord": "^1.13",
        "konekt/enum": "^4.1",
        "konekt/enum-eloquent": "^1.9",
        "konekt/xtend": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Konekt\\Address\\": "src",
            "Konekt\\Address\\Seeds\\": "src/resources/database/seeds"
        }
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit" : "^10.0",
        "mockery/mockery": "^1.6.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Address\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
