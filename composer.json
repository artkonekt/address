{
    "name": "konekt/address",
    "description": "Laravel module for handling countries, cities, addresses, people and organizations",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "support": {
        "issues": "https://github.com/artkonekt/address"
    },
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^2.13.3|^3.1.4",
        "konekt/concord": "^1.10.1",
        "konekt/enum": "^3.0|^4.0",
        "konekt/enum-eloquent": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Konekt\\Address\\": "src",
            "Konekt\\Address\\Seeds\\": "src/resources/database/seeds"
        }
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit" : "9 - 10",
        "mockery/mockery": "^1.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Konekt\\Address\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
